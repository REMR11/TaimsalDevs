@model SystemTaimsalDevs.EL.Rol

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>¿Estás seguro de eliminar?</h3>
<div>
    <h4>Rol</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nombre Rol
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NameRol)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdRol" />
        <button type="submit" value="Delete" class="btn btn-danger">Eliminar</button> |
        <a type="button" asp-action="Index" class="btn btn-primary">Volvel a la Lista</a>
    </form>
</div>

@model SystemTaimsalDevs.EL.Report

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Reporte</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Nombre Cliente
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdClientNavigation.NameClient)
        </dd>
        <dt class = "col-sm-2">
            Nombre Maquinaria
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdMachineNavigation.NameMachine)
        </dd>
        <dt class = "col-sm-2">
            Imagen Producto
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdProductNavigation.ImageProduct)
        </dd>
        <dt class = "col-sm-2">
            Nombre Proveedor
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdProviderNavigation.NameProvider)
        </dd>
        <dt class = "col-sm-2">
            Apellido Usuario
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdUserNavigation.LastNameUser)
        </dd>
    </dl>
</div>
<div>
    <a type="buttom" class="btn btn-success" asp-action="Edit" asp-route-id="@Model?.IdReport">Editar</a> |
    <a type="buttom" class="btn btn-primary" asp-action="Index">Volvel a la Lista</a>
</div>
